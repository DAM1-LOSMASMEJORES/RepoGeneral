package repogeneral;

/**
 *
 * @author Harold
 */
public class ejercicio_3 {
    public static void main(String[] args){
    for (int i = 1; i < 10; i++) {
            for (int j = 1; j < 10; j++) {
                int resultado;
                resultado=i*j;
                System.out.println(i+" * "+j+" = "+resultado );
                        
            }
        }
}
}
